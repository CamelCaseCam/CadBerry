endef
define POSTBUILDCMDS
endef

ifeq ($(config),debug)
TARGETDIR = ../bin/Debug-linux-x86_64/CadBerry_updater
TARGET = $(TARGETDIR)/CadBerry_updater
OBJDIR = ../bin/Debug-linux-x86_64/CadBerry_updater
DEFINES += -DCDB_PLATFORM_LINUX -DCDB_DEBUG
ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -g
ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -g
ALL_LDFLAGS += $(LDFLAGS) -L../CadBerry/vendor/cpr/lib/Debug -L/usr/lib64 -m64

